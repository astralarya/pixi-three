{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "./dist-typedoc/docs",
  "tsconfig": "./tsconfig.app.json",
  "name": "@astralarium/pixi-three",
  "readme": "./README.md",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeExternals": true,
  "categoryOrder": ["component", "hook", "*"],
  "kindSortOrder": [
    "Function",
    "Variable",
    "Class",
    "Interface",
    "TypeAlias",
    "Enum",
    "Reference"
  ],
  "navigation": {
    "includeCategories": true
  },
  "titleLink": "/pixi-three",
  "navigationLinks": {
    "GitHub": "https://github.com/astralarium/pixi-three"
  },
  "plugin": ["typedoc-rhineai-theme"]
}
